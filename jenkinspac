pipeline{
 agent any
 environment {
  my subect is = "k8s"
  batch = "1"
}

 parameters {
  string defaultValue: 'pac', description: 'PAC veriable', name: 'SUBJECT'
}

   stages{
     stage('woeking with pac'){
      steps{
       script{
          var1=20
           println "Hi team welcome to PAC"
           println " Hi your var1 value is ${var1}"
       }
     }
    }
    stage('Working with veriables')
    {
      steps{
        
          script{
          
            var1 = 301
             println "hi Your var1 value is ${var1}"
          }
        
      }
    }
  }
}
